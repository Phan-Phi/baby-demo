{
  "name": "baby-fish",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=true npm run build",
    "extract:intl": "formatjs extract --format simple {pages,components,libs}/**/*.{tsx,jsx,ts}  --out-file locales/original/vi.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
